    {
      "Id": "entry-filter.escape",
      "Options": {
        "EscapeDecoders": [
          {
            "Id": "escape-decoder.pattern",
            "Options": {
              "Patterns": [
                {
                  "Pattern": "(?<urls>http[^\\s,)]+)",
                  "Entries": ["C urls(l={urls})"]
