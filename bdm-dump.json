              ]
            }
          },
          {
            "Id": "escape-decoder.pattern",
            "Options": {
              "Patterns": [
                {
                  "Pattern": "\\@(?<a>[^,\\d]+),\\s*(?<w>[^;),]+),\\s*(?<l>[^;)]+)(?:;\\s*)?",
                  "Entries": ["C aref(a={a},w={w},l={l})"]
                }
              ]
            }
          },
          {
            "Id": "escape-decoder.pattern",
            "Options": {
              "Patterns": [
                {
                  "Pattern": "\\@(?<a>[^,\\d]+),\\s*(?<l>[^;)]+)(?:;\\s*)?",
                  "Entries": ["C aref(a={a},l={l})"]
                }
              ]
            }
          },
          {
            "Id": "escape-decoder.pattern",
            "Options": {
              "Patterns": [
                {
                  "Pattern": "\\@(?<a>[^,\\d]+)\\s+(?<y>[12]\\d{3})(,\\s*(?<l>[^;)]+)(?:;\\s*)?)?",
                  "Entries": ["C aref(a={a},y={y},l={l})"]
                }
              ]
            }
          },
          {
            "Id": "escape-decoder.pattern",
            "Options": {
              "Patterns": [
                {
                  "Pattern": "^\\s*&?\\s*\\[(?<t>[^\\]]+)\\]",
                  "Entries": ["C tags(t={t})"]
                }
              ]
            }
          }
        ]
      }
    }
  ],
